@model IEnumerable<AutoMate.Data.Entities.Vehicle>
@{
    ViewData["Title"] = "Vehicles";
}
<h2>All Vehicles</h2>
<ul>
    @foreach (var vehicle in Model) {
        <li>
            <a href="@Url.Action("Details", "Vehicles", new { id = vehicle.Registration })">@vehicle.Registration</a>
        </li>
    }
</ul>
